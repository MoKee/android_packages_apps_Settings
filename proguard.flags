-dontwarn com.tenpay.android.service.**
-keep class com.tenpay.android.service.** {*;}
-dontwarn com.unionpay.**
-keep class com.unionpay.** {*;}
-dontwarn com.UCMobile.**
-keep class com.UCMobile.** {*;}

-keep class com.mokee.helper.**
-keep class com.android.settings.mokee.mokeehelper.**

